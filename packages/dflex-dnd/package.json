{
  "name": "@dflex/dnd",
  "version": "3.8.0",
  "description": "Modern drag and drop package for all JavaScript frameworks",
  "author": "Jalal Maskoun",
  "main": "./dist/dflex-dnd.js",
  "module": "./dist/dflex-dnd.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "import": "./dist/dev.mjs",
        "require": "./dist/dev.js"
      },
      "require": "./dist/dflex-dnd.js",
      "import": "./dist/dflex-dnd.mjs"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "clean": "rimraf ./dist ./types tsconfig.tsbuildinfo",
    "emit": "tsc --emitDeclarationOnly",
    "compile:w": "tsc -b -w"
  },
  "homepage": "https://github.com/dflex-js/dflex/tree/master/packages/dnd",
  "repository": "https://github.com/dflex-js/dflex",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "LICENSE"
  ],
  "devDependencies": {
    "@dflex/core-instance": "workspace:^3.8.0",
    "@dflex/draggable": "workspace:^3.8.0",
    "@dflex/store": "workspace:^3.8.0",
    "@dflex/utils": "workspace:^3.8.0"
  },
  "keywords": [
    "drag-drop",
    "dnd",
    "sortable",
    "reorder",
    "drag",
    "drop",
    "DOM",
    "@dflex",
    "@dflex/dom-gen",
    "@dflex/store",
    "@dflex/core-instance",
    "@dflex/draggable",
    "@dflex/dnd"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
