{
  "name": "@dflex/dnd",
  "version": "3.5.1",
  "description": "Modern drag and drop package for all JavaScript frameworks",
  "author": "Jalal Maskoun",
  "main": "./dist/dflex-dnd.js",
  "module": "./dist/dflex-dnd.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/dflex-dnd.js",
      "import": "./dist/dflex-dnd.mjs"
    },
    "./production": "./dist/dflex-dnd.min.js"
  },
  "scripts": {
    "clean": "rimraf ./dist ./types tsconfig.tsbuildinfo",
    "emit": "tsc --emitDeclarationOnly",
    "build:cjs": "esbuild src/index.ts --bundle --format=cjs --outfile=dist/dflex-dnd.js",
    "build:esm": "esbuild src/index.ts  --sourcemap --bundle --format=esm --outfile=dist/dflex-dnd.mjs",
    "build:prod": "esbuild src/index.ts --bundle --format=cjs --minify --outfile=dist/dflex-dnd.min.js",
    "build": "pnpm clean && pnpm build:cjs && pnpm build:esm && pnpm build:prod && pnpm emit",
    "compile:w": "tsc -b -w",
    "build:w": "cross-env NODE_ENV=development pnpm build:esm -- --watch",
    "server": "pnpm -F dflex-dnd-playground dev",
    "cy:run": "cypress run --env extended=true",
    "cy:open": "cypress open --env extended=true",
    "cy:dev": "start-server-and-test 'pnpm run server' http-get://localhost:3001 'pnpm cy:open'",
    "test": "start-server-and-test 'pnpm run server' http-get://localhost:3001 'pnpm cy:run'",
    "cy:run:vertical:chrome": "cypress run --env extended=true --headless --spec 'cypress/integration/same-container-vertical/**/*.spec.ts' --config-file cypress.json --browser chrome",
    "cy:run:multi:chrome": "cypress run --env extended=true --headless --spec 'cypress/integration/multiple-containers/**/*.spec.ts' --config-file cypress.json --browser chrome",
    "test:vertical:chrome": "start-server-and-test 'pnpm run server' http-get://localhost:3001 'pnpm cy:run:vertical:chrome'",
    "test:multi:chrome": "start-server-and-test 'pnpm run server' http-get://localhost:3001 'pnpm cy:run:multi:chrome'"
  },
  "homepage": "https://github.com/dflex-js/dflex/tree/master/packages/dnd",
  "repository": "https://github.com/dflex-js/dflex",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "LICENSE"
  ],
  "devDependencies": {
    "@dflex/core-instance": "workspace:^3.5.1",
    "@dflex/draggable": "workspace:^3.5.1",
    "@dflex/store": "workspace:^3.5.1",
    "@dflex/utils": "workspace:^3.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.6.1",
    "eslint": "^8.15.0",
    "eslint-plugin-cypress": "^2.12.1",
    "start-server-and-test": "^1.14.0",
    "v8-to-istanbul": "^9.0.0"
  },
  "keywords": [
    "drag-drop",
    "dnd",
    "sortable",
    "reorder",
    "drag",
    "drop",
    "DOM",
    "@dflex",
    "@dflex/dom-gen",
    "@dflex/store",
    "@dflex/core-instance",
    "@dflex/draggable",
    "@dflex/dnd"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
