{
  "name": "@dflex/core-instance",
  "version": "3.9.3",
  "description": "Core instance is a DFlex package for interactive DOM element",
  "author": "Jalal Maskoun",
  "main": "./dist/dflex-core.js",
  "module": "./dist/dflex-core.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "import": "./dist/dev.mjs",
        "require": "./dist/dev.js"
      },
      "require": "./dist/dflex-core.js",
      "import": "./dist/dflex-core.mjs"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "clean": "rimraf ./dist ./types tsconfig.tsbuildinfo",
    "emit": "tsc --emitDeclarationOnly"
  },
  "repository": "https://github.com/dflex-js/dflex",
  "homepage": "https://github.com/dflex-js/dflex/tree/main/packages/dflex-core-instance",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "LICENSE"
  ],
  "devDependencies": {
    "@dflex/utils": "workspace:^3.9.3"
  },
  "keywords": [
    "drag-drop",
    "dnd",
    "sortable",
    "reorder",
    "drag",
    "drop",
    "DOM",
    "@dflex",
    "@dflex/dom-gen",
    "@dflex/store",
    "@dflex/core-instance",
    "@dflex/draggable",
    "@dflex/dnd"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
