{
  "name": "@dflex/dnd",
  "version": "3.3.2",
  "description": "Modern drag and drop package for all JavaScript frameworks",
  "author": "Jalal Maskoun",
  "main": "./dist/dflex.js",
  "module": "./dist/dflex.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dflex.js",
      "require": "./dist/dflex.mjs",
      "types": "./dist/index.d.ts"
    },
    "./production": "./dist/dflex.min.js"
  },
  "scripts": {
    "clean": "yarn rimraf ./dist tsconfig.tsbuildinfo",
    "compile": "yarn clean && yarn tsc -b",
    "emit": "tsc --emitDeclarationOnly",
    "build:cjs": "esbuild src/index.ts --bundle --external:@dflex  --format=cjs --outfile=dist/dflex.js",
    "build:esm": "esbuild src/index.ts --bundle --external:@dflex  --format=esm --outfile=dist/dflex.mjs",
    "build:prod": "esbuild src/index.ts --bundle --format=cjs --minify --outfile=dist/dflex.min.js",
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:prod && yarn emit",
    "server:3001": "cross-env BROWSER=none yarn workspace dflex-react-dnd start",
    "cy:run": "cypress run",
    "cy:run:extended": "cypress run --env extended=true",
    "cy:open": "cypress open",
    "cy:open:extended": "cypress open --env extended=true",
    "dev": "yarn workspace dflex-react-dnd start",
    "dev:cy": "start-server-and-test server:3001 http://localhost:3001 cy:open",
    "dev:cy:extended": "start-server-and-test server:3001 http://localhost:3001 cy:open:extended",
    "test": "cross-env NODE_ENV=e2e start-server-and-test server:3001 http://localhost:3001 cy:run",
    "test:extended": "start-server-and-test server:3001 http://localhost:3001 cy:run:extended"
  },
  "homepage": "https://github.com/dflex-js/dflex/tree/master/packages/dnd",
  "repository": "https://github.com/dflex-js/dflex",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE"
  ],
  "devDependencies": {
    "cross-env": "^7.0.2",
    "cypress": "^9.5.0",
    "eslint-plugin-cypress": "^2.11.1",
    "start-server-and-test": "^1.11.2",
    "v8-to-istanbul": "^8.0.0"
  },
  "dependencies": {
    "@dflex/utils": "^3.3.2",
    "@dflex/core-instance": "^3.3.2",
    "@dflex/draggable": "^3.3.2",
    "@dflex/store": "^3.3.2"
  },
  "keywords": [
    "drag-drop",
    "dnd",
    "sortable",
    "reorder",
    "drag",
    "drop",
    "DOM",
    "front-end",
    "@dflex",
    "@dflex/dom-gen",
    "@dflex/store",
    "@dflex/core-instance",
    "@dflex/draggable",
    "@dflex/dnd"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
