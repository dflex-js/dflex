{
  "name": "@dflex/dnd",
  "version": "3.3.2",
  "description": "Modern drag and drop package for all JavaScript frameworks",
  "author": "Jalal Maskoun",
  "main": "./dist/dflex-dnd.js",
  "module": "./dist/dflex-dnd.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/dflex-dnd.js",
      "import": "./dist/dflex-dnd.mjs",
      "types": "./dist/index.d.ts"
    },
    "./production": "./dist/dflex-dnd.min.js"
  },
  "scripts": {
    "clean": "pnpm rimraf ./dist tsconfig.tsbuildinfo",
    "emit": "tsc --emitDeclarationOnly",
    "build:cjs": "esbuild src/index.ts --bundle --format=cjs --outfile=dist/dflex-dnd.js",
    "build:esm": "esbuild src/index.ts --bundle --format=esm --outfile=dist/dflex-dnd.mjs",
    "build:prod": "esbuild src/index.ts --bundle --format=cjs --minify --outfile=dist/dflex-dnd.min.js",
    "build": "pnpm clean && pnpm build:cjs && pnpm build:esm && pnpm build:prod && pnpm emit",
    "compile:w": "tsc -b -w",
    "build:w": "pnpm clean && pnpm emit && pnpm build:cjs --define:process.env.NODE_ENV=\\\"development\\\" --watch",
    "server:3001": "cross-env BROWSER=none pnpm dev --filter dflex-react-dnd",
    "dev:3001": "pnpm dev --filter dflex-react-dnd",
    "cy:run": "cypress run --env extended=true",
    "cy:open": "cypress open --env extended=true",
    "cy:dev": "start-server-and-test server:3001 http://localhost:3001 cy:open",
    "test": "start-server-and-test server:3001 http://localhost:3001 cy:run"
  },
  "homepage": "https://github.com/dflex-js/dflex/tree/master/packages/dnd",
  "repository": "https://github.com/dflex-js/dflex",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE"
  ],
  "devDependencies": {
    "@dflex/utils": "^3.3.2",
    "@dflex/core-instance": "^3.3.2",
    "@dflex/draggable": "^3.3.2",
    "@dflex/store": "^3.3.2",
    "cross-env": "^7.0.2",
    "cypress": "^9.5.0",
    "eslint-plugin-cypress": "^2.11.1",
    "start-server-and-test": "^1.11.2",
    "v8-to-istanbul": "^8.0.0"
  },
  "dependencies": {},
  "keywords": [
    "drag-drop",
    "dnd",
    "sortable",
    "reorder",
    "drag",
    "drop",
    "DOM",
    "front-end",
    "@dflex",
    "@dflex/dom-gen",
    "@dflex/store",
    "@dflex/core-instance",
    "@dflex/draggable",
    "@dflex/dnd"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
