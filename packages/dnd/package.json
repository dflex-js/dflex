  "files": [
    "src",
    "dist",
    "LICENSE"
  ],
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.11.0",
    "dflex-utils-test": "^0.1.0",
    "eslint-plugin-cypress": "^2.11.1",
    "start-server-and-test": "^1.11.2"
  },
  "bundledDependencies": [
    "@dflex/store",
    "@dflex/core-instance",
    "@dflex/draggable",
    "@dflex/event"
  ],
  "keywords": [
    "@dflex",
